    "license": "MIT",
    "authors": [
        {
            "name": "BKV1409",
            "email": "bkv1409@gmail.com",
            "homepage": "http://author.com"
        }
    ],
    "homepage": "https://github.com/bkv1409/system-config",
    "keywords": ["Laravel", "SystemConfig"],
    "require": {
        "illuminate/support": "~8",
        "spatie/laravel-activitylog": "^3.17",
        "spatie/laravel-permission": "^5.4"
    },
    "require-dev": {
        "phpunit/phpunit": "~8.0",
        "orchestra/testbench": "~6.24.1"
    },
    "autoload": {
